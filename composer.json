{
    "name": "roadiz/models",
    "description": "Roadiz base models for entities.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "roadiz",
        "cms",
        "backoffice",
        "rezo zero"
    ],
    "authors": [
        {
            "name": "Ambroise Maupate",
            "email": "ambroise@roadiz.io",
            "homepage": "http://www.roadiz.io",
            "role": "Lead developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/orm": "~2.7.5 || ~2.8.1",
        "pimple/pimple": "^3.3.1",
        "jms/serializer": "^2.3.0 || ^3.9.0",
        "symfony/string": "^5.0",
        "symfony/translation-contracts": "^2.3",
        "symfony/http-foundation": "^5.0.7",
        "symfony/serializer": "^5.0.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4.3",
        "php-coveralls/php-coveralls": "^2.4.2",
        "squizlabs/php_codesniffer": "^3.5",
        "phpstan/phpstan": "^0.12.57"
    },
    "autoload": {
        "psr-4": {
